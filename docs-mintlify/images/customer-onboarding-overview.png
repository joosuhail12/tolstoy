<svg width="700" height="500" xmlns="http://www.w3.org/2000/svg">
  <rect width="700" height="500" fill="#f8f9fa" stroke="#dee2e6" stroke-width="1"/>
  <text x="350" y="25" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#333">Customer Onboarding Journey</text>
  
  <!-- New Customer -->
  <rect x="50" y="80" width="120" height="60" fill="#e3f2fd" stroke="#1976d2" stroke-width="2" rx="8"/>
  <text x="110" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1976d2">New Customer</text>
  <text x="110" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#1976d2">Registration</text>
  
  <!-- Onboarding Engine -->
  <rect x="250" y="160" width="200" height="80" fill="#f3e5f5" stroke="#7b1fa2" stroke-width="3" rx="10"/>
  <text x="350" y="185" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#7b1fa2">Tolstoy Onboarding</text>
  <text x="350" y="205" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#7b1fa2">Personalized Journey</text>
  <text x="350" y="225" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#7b1fa2">Multi-step Automation</text>
  
  <!-- Onboarding Steps -->
  <rect x="50" y="300" width="130" height="50" fill="#fff3e0" stroke="#f57c00" stroke-width="2" rx="6"/>
  <text x="115" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#f57c00">1. Welcome</text>
  <text x="115" y="335" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#f57c00">Email & SMS</text>
  
  <rect x="200" y="300" width="130" height="50" fill="#e8f5e8" stroke="#388e3c" stroke-width="2" rx="6"/>
  <text x="265" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#388e3c">2. Setup</text>
  <text x="265" y="335" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#388e3c">Profile & Preferences</text>
  
  <rect x="350" y="300" width="130" height="50" fill="#e8eaf6" stroke="#3f51b5" stroke-width="2" rx="6"/>
  <text x="415" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#3f51b5">3. Activate</text>
  <text x="415" y="335" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#3f51b5">Account Verification</text>
  
  <rect x="500" y="300" width="130" height="50" fill="#fce4ec" stroke="#ad1457" stroke-width="2" rx="6"/>
  <text x="565" y="320" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#ad1457">4. Engage</text>
  <text x="565" y="335" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#ad1457">Tutorial & Tips</text>
  
  <!-- Communication Channels -->
  <rect x="100" y="400" width="100" height="40" fill="#c8e6c9" stroke="#4caf50" stroke-width="2" rx="5"/>
  <text x="150" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#4caf50">Email</text>
  <text x="150" y="432" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#4caf50">Personalized</text>
  
  <rect x="220" y="400" width="100" height="40" fill="#ffcdd2" stroke="#f44336" stroke-width="2" rx="5"/>
  <text x="270" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#f44336">SMS</text>
  <text x="270" y="432" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f44336">Time-sensitive</text>
  
  <rect x="340" y="400" width="100" height="40" fill="#b3e5fc" stroke="#03a9f4" stroke-width="2" rx="5"/>
  <text x="390" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#03a9f4">In-App</text>
  <text x="390" y="432" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#03a9f4">Guided tours</text>
  
  <rect x="460" y="400" width="100" height="40" fill="#d1c4e9" stroke="#9c27b0" stroke-width="2" rx="5"/>
  <text x="510" y="420" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#9c27b0">Push</text>
  <text x="510" y="432" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#9c27b0">Mobile alerts</text>
  
  <!-- Success Metrics -->
  <rect x="520" y="80" width="130" height="80" fill="#e0f2f1" stroke="#00695c" stroke-width="2" rx="8"/>
  <text x="585" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="13" font-weight="bold" fill="#00695c">Success Metrics</text>
  <text x="585" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#00695c">• Profile Completion</text>
  <text x="585" y="140" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#00695c">• First Action Taken</text>
  <text x="585" y="155" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#00695c">• Time to Value</text>
  
  <!-- Personalization Engine -->
  <rect x="50" y="180" width="150" height="60" fill="#fff9c4" stroke="#f9a825" stroke-width="2" rx="6"/>
  <text x="125" y="200" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#f9a825">Personalization</text>
  <text x="125" y="215" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f9a825">• User Behavior</text>
  <text x="125" y="230" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f9a825">• Industry Segment</text>
  
  <!-- Flow Arrows -->
  <path d="M 170 110 L 250 200" stroke="#666" stroke-width="3" marker-end="url(#arrowhead)"/>
  <text x="210" y="155" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#666">triggers</text>
  
  <path d="M 200 200 L 125 200" stroke="#f9a825" stroke-width="2" stroke-dasharray="5,5"/>
  <text x="162" y="195" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f9a825">personalizes</text>
  
  <path d="M 350 240 L 115 300" stroke="#666" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 350 240 L 265 300" stroke="#666" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 350 240 L 415 300" stroke="#666" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 350 240 L 565 300" stroke="#666" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <path d="M 150 350 L 150 400" stroke="#4caf50" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 270 350 L 270 400" stroke="#f44336" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 390 350 L 390 400" stroke="#03a9f4" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 510 350 L 510 400" stroke="#9c27b0" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <path d="M 450 200 L 520 120" stroke="#00695c" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Timeline -->
  <rect x="50" y="460" width="600" height="25" fill="#f5f5f5" stroke="#ccc" stroke-width="1" rx="3"/>
  <text x="60" y="477" font-family="Arial, sans-serif" font-size="10" fill="#666">Day 0</text>
  <text x="200" y="477" font-family="Arial, sans-serif" font-size="10" fill="#666">Day 1-3</text>
  <text x="380" y="477" font-family="Arial, sans-serif" font-size="10" fill="#666">Day 7</text>
  <text x="580" y="477" font-family="Arial, sans-serif" font-size="10" fill="#666">Day 30</text>
  
  <!-- Progress indicators -->
  <circle cx="115" cy="472" r="4" fill="#4caf50"/>
  <circle cx="265" cy="472" r="4" fill="#ff9800"/>
  <circle cx="415" cy="472" r="4" fill="#2196f3"/>
  <circle cx="565" cy="472" r="4" fill="#9c27b0"/>
  
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="10" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#666"/>
    </marker>
  </defs>
</svg>