<svg width="650" height="500" xmlns="http://www.w3.org/2000/svg">
  <rect width="650" height="500" fill="#f9f9f9" stroke="#e0e0e0" stroke-width="1"/>
  <text x="325" y="25" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#333">Multi-Channel Notification System</text>
  
  <!-- Event Source -->
  <rect x="50" y="80" width="120" height="60" fill="#e3f2fd" stroke="#1976d2" stroke-width="2" rx="8"/>
  <text x="110" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1976d2">Event Source</text>
  <text x="110" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" fill="#1976d2">Trigger Events</text>
  
  <!-- Notification Engine -->
  <rect x="250" y="160" width="150" height="80" fill="#f3e5f5" stroke="#7b1fa2" stroke-width="3" rx="10"/>
  <text x="325" y="185" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#7b1fa2">Tolstoy Notification</text>
  <text x="325" y="205" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#7b1fa2">Routing Engine</text>
  <text x="325" y="225" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#7b1fa2">Smart Delivery</text>
  
  <!-- Channel Selection Logic -->
  <rect x="480" y="80" width="120" height="80" fill="#fff3e0" stroke="#f57c00" stroke-width="2" rx="8"/>
  <text x="540" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="13" font-weight="bold" fill="#f57c00">Channel Logic</text>
  <text x="540" y="125" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f57c00">• Priority Rules</text>
  <text x="540" y="140" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f57c00">• User Preferences</text>
  <text x="540" y="155" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f57c00">• Fallback Logic</text>
  
  <!-- Notification Channels -->
  <rect x="50" y="320" width="100" height="50" fill="#e8f5e8" stroke="#388e3c" stroke-width="2" rx="6"/>
  <text x="100" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#388e3c">Email</text>
  <text x="100" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#388e3c">Rich Content</text>
  
  <rect x="170" y="320" width="100" height="50" fill="#ffebee" stroke="#d32f2f" stroke-width="2" rx="6"/>
  <text x="220" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#d32f2f">SMS</text>
  <text x="220" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#d32f2f">Urgent Alerts</text>
  
  <rect x="290" y="320" width="100" height="50" fill="#e8eaf6" stroke="#3f51b5" stroke-width="2" rx="6"/>
  <text x="340" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#3f51b5">Push</text>
  <text x="340" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#3f51b5">Mobile Apps</text>
  
  <rect x="410" y="320" width="100" height="50" fill="#fce4ec" stroke="#ad1457" stroke-width="2" rx="6"/>
  <text x="460" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#ad1457">Slack</text>
  <text x="460" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#ad1457">Team Channels</text>
  
  <rect x="530" y="320" width="100" height="50" fill="#f1f8e9" stroke="#689f38" stroke-width="2" rx="6"/>
  <text x="580" y="340" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#689f38">Webhook</text>
  <text x="580" y="355" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#689f38">Custom Systems</text>
  
  <!-- Message Templates -->
  <rect x="100" y="420" width="120" height="50" fill="#fff9c4" stroke="#f9a825" stroke-width="2" rx="5"/>
  <text x="160" y="440" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#f9a825">Templates</text>
  <text x="160" y="455" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f9a825">Dynamic Content</text>
  
  <!-- Delivery Tracking -->
  <rect x="280" y="420" width="120" height="50" fill="#e0f2f1" stroke="#00695c" stroke-width="2" rx="5"/>
  <text x="340" y="440" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#00695c">Tracking</text>
  <text x="340" y="455" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#00695c">Delivery Status</text>
  
  <!-- Analytics -->
  <rect x="460" y="420" width="120" height="50" fill="#b3e5fc" stroke="#03a9f4" stroke-width="2" rx="5"/>
  <text x="520" y="440" text-anchor="middle" font-family="Arial, sans-serif" font-size="11" font-weight="bold" fill="#03a9f4">Analytics</text>
  <text x="520" y="455" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#03a9f4">Open & Click Rates</text>
  
  <!-- Event Types -->
  <rect x="50" y="200" width="80" height="30" fill="#c8e6c9" stroke="#4caf50" stroke-width="1" rx="3"/>
  <text x="90" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#4caf50">Welcome</text>
  
  <rect x="50" y="240" width="80" height="30" fill="#ffcdd2" stroke="#f44336" stroke-width="1" rx="3"/>
  <text x="90" y="260" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f44336">Alert</text>
  
  <rect x="50" y="280" width="80" height="30" fill="#fff3e0" stroke="#ff9800" stroke-width="1" rx="3"/>
  <text x="90" y="300" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#ff9800">Reminder</text>
  
  <!-- Flow Arrows -->
  <path d="M 170 110 L 250 200" stroke="#666" stroke-width="3" marker-end="url(#arrowhead)"/>
  <text x="210" y="155" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#666">triggers</text>
  
  <path d="M 400 200 L 480 120" stroke="#f57c00" stroke-width="2" marker-end="url(#arrowhead)"/>
  <text x="440" y="160" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#f57c00">selects channels</text>
  
  <path d="M 300 240 L 100 320" stroke="#388e3c" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 320 240 L 220 320" stroke="#d32f2f" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 340 240 L 340 320" stroke="#3f51b5" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 360 240 L 460 320" stroke="#ad1457" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M 380 240 L 580 320" stroke="#689f38" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <path d="M 160 370 L 160 420" stroke="#f9a825" stroke-width="1" marker-end="url(#arrowhead)"/>
  <path d="M 340 370 L 340 420" stroke="#00695c" stroke-width="1" marker-end="url(#arrowhead)"/>
  <path d="M 520 370 L 520 420" stroke="#03a9f4" stroke-width="1" marker-end="url(#arrowhead)"/>
  
  <!-- Connected arrows from event types -->
  <path d="M 130 215 L 250 200" stroke="#4caf50" stroke-width="1" stroke-dasharray="3,3"/>
  <path d="M 130 255 L 250 210" stroke="#f44336" stroke-width="1" stroke-dasharray="3,3"/>
  <path d="M 130 295 L 250 220" stroke="#ff9800" stroke-width="1" stroke-dasharray="3,3"/>
  
  <defs>
    <marker id="arrowhead" markerWidth="8" markerHeight="6" refX="8" refY="3" orient="auto">
      <polygon points="0 0, 8 3, 0 6" fill="#666"/>
    </marker>
  </defs>
</svg>